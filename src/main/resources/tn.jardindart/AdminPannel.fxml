                                        <TableColumn fx:id="points" prefWidth="75.0" text="POINTS" />
                                        <TableColumn fx:id="dn" prefWidth="75.0" text="BIRTHDAY" />
                                        <TableColumn fx:id="status" prefWidth="75.0" text="STATUS" />
                                    </columns>
                                </TableView>
                            </children>
