                                <TextField fx:id="filterField" layoutX="566.0" layoutY="73.0" prefHeight="27.0" prefWidth="183.0" promptText="search" style="-fx-background-color: #EBE8F9; -fx-border-color: #B7C3D7;" />
                                <Label layoutX="45.0" layoutY="69.0" text="Admin Panel">
                                    <font>
                                        <Font size="24.0" />
                                    </font>
                                </Label>
                                <ImageView fx:id="SearchButtonUserClick" fitHeight="17.0" fitWidth="16.0" layoutX="722.0" layoutY="79.0" pickOnBounds="true" preserveRatio="true">
                                    <image>
                                        <Image url="@../images/icons8_Search_52px.png" />
                                    </image>
                                </ImageView>
                                <TableView fx:id="tableView" layoutX="35.0" layoutY="144.0" prefHeight="359.0" prefWidth="781.0">
