<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox prefHeight="600" prefWidth="800" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="tn.esprit.jardindart.controllers.DA.AfficherDA">

    <TableView fx:id="tableac">
        <columns>
            <!-- ID column -->
            <TableColumn fx:id="idColumn" prefWidth="100" text="ID">
                <!-- Define how to display IDs -->
            </TableColumn>

            <!-- Montant column -->
            <TableColumn fx:id="montantColumn" prefWidth="100" text="Montant">
                <!-- Define how to display montant -->
            </TableColumn>

            <!-- Association column -->
            <TableColumn fx:id="associationColumn" prefWidth="150" text="Association">
                <!-- Define how to display association -->
            </TableColumn>

            <!-- User ID column -->
            <TableColumn fx:id="userIdColumn" prefWidth="150" text="User ID">
                <!-- Define how to display user ID -->
            </TableColumn>
        </columns>
    </TableView>


</VBox>
